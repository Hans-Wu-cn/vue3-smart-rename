{
  "name": "vue3-smart-rename",
  "displayName": "Vue3 Smart Rename",
  "description": "An intelligent rename extension for Vue 3 projects that automatically synchronizes updates of component props, functions, and variable names",
  "version": "0.0.1",
  "publisher": "cn-hansWu",
  "repository": {
    "type": "git",
    "url": "https://github.com/cn-hansWu/vue3-smart-rename.git"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "activationEvents": [
    "onCommand:vue3-smart-rename.smartRename"
  ],
  "main": "out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vue3-smart-rename.smartRename",
        "title": "智能同步更新变量/函数名"
      }
    ],
    "keybindings": [
      {
        "command": "vue3-smart-rename.smartRename",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.70.0",
    "@vscode/vsce": "^3.7.1",
    "typescript": "^4.8.4"
  },
  "keywords": [
    "vscode",
    "extension",
    "vue",
    "vue3",
    "rename",
    "refactor",
    "props",
    "smart-rename",
    "sync"
  ],
  "author": "",
  "license": "MIT"
}
